[core]
	autocrlf = input
	excludesfile = /Users/tamouse/.gitignore_global
[color]
	ui = true
[push]
	default = matching
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path =
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	aa = add --all
	cim = commit --message
	pufu = push -fu
	difnam = diff --name-only
	difnmst = diff --name-status
	co = checkout
	ci = commit
	dryrun = merge --no-commit --no-ff --no-edit
	cine = commit --no-edit
[commit]
[pull]
	rebase = false
[init]
	defaultBranch = main
