[user]
	name = Tamara Temple
	email = tamara@mrets.org
[core]
	autocrlf = input
	excludesfile = /Users/tamara/.gitignore_global
[color]
	ui = true
[push]
	default = matching
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[alias]
	aa = add --all
	cim = commit --message
	pufu = push -fu
	difnam = diff --name-only
	difnmst = diff --name-status
	co = checkout
	ci = commit
	dryrun = merge --no-commit --no-ff --no-edit
	cine = commit --no-edit
[commit]
[pull]
	rebase = false
[init]
	defaultBranch = main
